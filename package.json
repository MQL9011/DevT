{
  "name": "devtool",
  "version": "1.0.0",
  "description": "DevTool - 开发者工具集合",
  "private": true,
  "workspaces": [
    "frontend",
    "backend"
  ],
  "scripts": {
    "dev": "concurrently \"pnpm --filter backend start:dev\" \"pnpm --filter frontend dev\"",
    "dev:frontend": "pnpm --filter frontend dev",
    "dev:backend": "pnpm --filter backend start:dev",
    "build": "pnpm -r build",
    "build:frontend": "pnpm --filter frontend build",
    "build:frontend:pack": "pnpm --filter frontend build:pack",
    "build:backend": "pnpm --filter backend build",
    "start": "concurrently \"pnpm --filter backend start:prod\" \"pnpm --filter frontend start\"",
    "lint": "pnpm -r lint",
    "install:all": "pnpm install"
  },
  "devDependencies": {
    "concurrently": "^9.1.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "packageManager": "pnpm@10.18.2+sha512.9fb969fa749b3ade6035e0f109f0b8a60b5d08a1a87fdf72e337da90dcc93336e2280ca4e44f2358a649b83c17959e9993e777c2080879f3801e6f0d999ad3dd"
}
